    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.194",
    "apollo-errors": "^1.9.0",
    "arangojs": "^6.11.0",
    "bcrypt": "^5.1.0",
    "dataloader": "^1.4.0",
